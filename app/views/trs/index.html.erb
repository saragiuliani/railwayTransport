<h1>Listing trs</h1>

<table>
  <tr>
    <th>Railroute departure</th>
    <th>Railroute destination</th>
    <th>Railroute dtime</th>
    <th>Train</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @trs.each do |tr| %>
  <tr>
    <td><%= tr.railroute_departure %></td>
    <td><%= tr.railroute_destination %></td>
    <td><%= tr.railroute_dtime %></td>
    <td><%= tr.train %></td>
    <td><%= link_to 'Show', tr %></td>
    <td><%= link_to 'Edit', edit_tr_path(tr) %></td>
    <td><%= link_to 'Destroy', tr, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

